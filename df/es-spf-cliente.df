  FORMAT "x(30)"
  INITIAL ""
  LABEL "Codigo Shopify"
  POSITION 3
  MAX-WIDTH 80
  COLUMN-LABEL "Codigo Shopify"
  ORDER 20


ADD INDEX "ch-emitente" ON "es-spf-cliente" 
  AREA "Schema Area"
  UNIQUE
  PRIMARY
  INDEX-FIELD "cod-emitente" ASCENDING 
